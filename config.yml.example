# aws
aws_output_format: "json"
aws_region: "ap-northeast-1"
aws_access_key_id: "YOUR_ACCESS_KEY_ID"
aws_secret_access_key: "YOUR_SECERT_ACCESS_KEY"
install_features:
  - timezone
  - ntp
  - swap
  - selinux
  - yum_cron
  - firewalld
  - common
  - nginx
  - php-fpm
  - composer
  - mysql
  - drush
  - drupal
  - awscli
  - nodejs
  # - openjdk
  # - solr4drupal
  - cron
firewalld_allowed_ports:
  - "22/tcp"
  - "80/tcp"
php_version: "7.0"
php_fpm_php_session_save_path: /var/lib/php/session
#php_fpm_php_session_save_path: /var/opt/rh/rh-php70/lib/php/session
drupal_site_install: true
drupal_root_path: "/var/www/example.com"
drupal_extra_modules:
  - devel
drupal_enable_modules:
  - devel
